prn_setup(){ sudo lpadmin -p "${1:?Printer name not specified}" -E -v "${2:?Printer URL not specified}" -P "${3:?PPD file path not specified}";};prn_delete(){ local NAME="${1:?Printer name not specified}";sudo lpoptions -x "$NAME";sudo lpadmin -x "$NAME";};prn_options(){ local NAME="${1:?Printer name not specified}";local OPTS="";shift;for VALUES;do OPTS="$OPTS $VALUES";done;sudo lpoptions -p "$NAME" $OPTS;};alias prn_ls='lpstat -t';alias prn_default='lpoptions -d';alias prn_print='lp';alias prn_enable='cupsenable';alias prn_disable='cupsdisable';alias prn_stat='lpstat';alias prn_id='lpq';alias prn_move='lpmove';alias prn_cancel='cancel'