if command -v tmux>/dev/null;then if [ -z "$TMUX" -a -n "$SSH_CONNECTION" -a "${TMUX_AUTOLOAD#*ssh}" != "$TMUX_AUTOLOAD" ];then TMUX_AUTOLOAD="yes";fi;tmux(){ if [ $# = 0 ];then command -p tmux attach -d||command -p tmux new-session;else command -p tmux "$@";fi;};alias tmux_ls='tmux ls 2>/dev/null';alias tmux_attach='reptyr';alias tmux_kill='tmux kill-session';if [ -z "$TMUX" -a -z "$TMUX_LOADED" ];then if [ "$TMUX_AUTOLOAD" = "yes" ]&&shell_isinteractive&&shell_islogin;then tmux 2>/dev/null;fi;fi;export TMUX_LOADED=1;[ "${1#tmux}" != "$1" ]&&"$@"||true;fi